{
    "name": "@nathanstitt/sundry",
    "private": false,
    "sideEffects": false,
    "version": "0.0.1",
    "main": "./dist/sundry.cjs",
    "module": "./dist/sundry.mjs",
    "scripts": {
        "dev": "vite --host",
        "pf": "run-p typecheck lint",
        "lint": "eslint src",
        "typecheck": "tsc --noemit",
        "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
        "test": "jest --runInBand",
        "test:coverage": "jest --runInBand --coverage",
        "lint:scripts": "eslint . --ext .ts",
        "lint:styles": "stylelint ./**/*.{css,scss}",
        "format:scripts": "prettier . --write",
        "format:styles": "stylelint ./**/*.{css,scss} --fix",
        "format": "npm run format:scripts && npm run format:styles",
        "prepare": "husky install && husky add .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit"
    },
    "peerDependencies": {
        "@emotion/css": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "react": "^18.1.0"
    },
    "devDependencies": {
        "@types/jest": "^29.0.0",
        "@types/jsdom": "^20.0.0",
        "@types/node": "^18.7.16",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.3",
        "@typescript-eslint/eslint-plugin": "^5.36.2",
        "@typescript-eslint/parser": "^5.36.2",
        "dts-bundle-generator": "^6.13.0",
        "eslint": "^8.23.0",
        "@emotion/css": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "husky": "^8.0.1",
        "jest": "^28.1.3",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "stylelint": "^14.11.0",
        "stylelint-config-recommended": "^9.0.0",
        "stylelint-config-sass-guidelines": "^9.0.1",
        "ts-jest": "^28.0.8",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.3",
        "vite": "^3.1.0"
    },
    "dependencies": {
        "@iconify-icons/bi": "^1.2.10",
        "@iconify/react": "^4.0.0",
        "@popperjs/core": "^2.11.6",
        "@react-types/shared": "^3.15.0",
        "@types/flatpickr": "^3.1.2",
        "@types/lodash-es": "^4.17.6",
        "@types/yup": "^0.32.0",
        "boxible": "^1.7.0",
        "dayjs": "^1.11.6",
        "eslint-plugin-react-hooks": "^4.6.0",
        "flatpickr": "^4.6.13",
        "formik": "^2.2.9",
        "graphql": "^16.6.0",
        "lodash-es": "^4.17.21",
        "react-popper": "^2.3.0",
        "react-select": "^5.5.7",
        "rooks": "^7.4.1",
        "urql": "^3.0.3",
        "vite-plugin-external": "^1.2.6"
    }
}
