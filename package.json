{
    "name": "@nathanstitt/sundry",
    "private": false,
    "sideEffects": false,
    "version": "0.0.15",
    "main": "./dist/sundry.cjs",
    "module": "./dist/sundry.mjs",
    "exports": {
        "./src": {
            "require": "./dist/index.js",
            "import": "./dist/index.js"
        },
        ".": {
            "require": "./dist/sundry.cjs",
            "import": "./dist/sundry.mjs"
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "typings": "dist/index.d.ts",
    "scripts": {
        "dev": "vite --host",
        "pf": "run-p typecheck lint",
        "lint": "eslint src",
        "typecheck": "tsc --noemit",
        "build": "vite build && tsc",
        "test": "jest --runInBand",
        "ci": "jest --ci",
        "test:coverage": "jest --runInBand --coverage",
        "lint:scripts": "eslint . --ext .ts",
        "lint:styles": "stylelint ./**/*.{css,scss}",
        "format:scripts": "prettier . --write",
        "prepublishOnly": "run-p build",
        "format:styles": "stylelint ./**/*.{css,scss} --fix",
        "format": "run-p format:scripts format:styles"
    },
    "peerDependencies": {
        "@emotion/css": "^11.10",
        "@emotion/styled": "^11.10",
        "react": "^18.2"
    },
    "devDependencies": {
        "@emotion/css": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@react-types/shared": "^3.15.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.2.2",
        "@types/jsdom": "^20.0.1",
        "@types/node": "^18.11.9",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.8",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "babel-jest": "^29.3.0",
        "babel-preset-env": "^1.7.0",
        "bootstrap": "^5.2.2",
        "dts-bundle-generator": "^7.0.0",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.1",
        "jest": "^29.3.0",
        "lint-staged": "^13.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.7.1",
        "react": "^18.1.0",
        "react-dom": "^18.2.0",
        "react-select-event": "^5.5.1",
        "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
        "stylelint": "^14.14.1",
        "stylelint-config-recommended": "^9.0.0",
        "stylelint-config-sass-guidelines": "^9.0.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "vite": "^3.2.3",
        "vite-plugin-external": "^1.2.6"
    },
    "dependencies": {
        "@hookform/resolvers": "^2.9",
        "@iconify/icons-bi": "^1.2",
        "@iconify/react": "^4.0",
        "@popperjs/core": "^2.11",
        "@restart/ui": "^1.4",
        "boxible": "^1.7",
        "dayjs": "*",
        "flatpickr": "^4.6",
        "graphql": "*",
        "react-hook-form": "^7.39",
        "react-popper": "^2.3",
        "react-select": "^5.6",
